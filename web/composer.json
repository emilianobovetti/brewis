{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.8.*",
        "phpseclib/phpseclib": "~3.0",
        "tymon/jwt-auth": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "fzaninotto/faker": "~1.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "database/migrations"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    }
}
